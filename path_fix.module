<?php

function path_fix_pathauto_alias_alter(&$alias, array &$context) {
  $alias =  str_replace("æ","a", $alias );
  $alias =  str_replace("ø","o", $alias );
  $alias =  str_replace("å","a", $alias );
  $alias =  str_replace("Æ","Æ", $alias );
  $alias =  str_replace("Ø","Ø", $alias );
  $alias =  str_replace("Å","Å", $alias );
}
